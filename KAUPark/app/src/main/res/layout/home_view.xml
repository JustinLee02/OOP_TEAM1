<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.HomeView">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/hometoolbar"
        android:layout_width="match_parent"
        android:layout_height="72dp"
        android:layout_marginTop="0dp"
        android:background="#40368A"
        app:layout_constraintTop_toTopOf="@id/frameLayout2"></androidx.appcompat.widget.Toolbar>

    <ScrollView
        android:id="@+id/scrollview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:fillViewport="true"
        android:scrollbars="vertical"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/hometoolbar">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/secondconst"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp"
            app:layout_constraintStart_toStartOf="@id/scrollview"
            app:layout_constraintTop_toTopOf="@+id/scrollview"

            >

            <TextView
                android:id="@+id/myInfo"
                android:layout_width="61dp"
                android:layout_height="30dp"
                android:layout_marginStart="18dp"
                android:layout_marginTop="31dp"
                android:text="내 정보"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/secondconst" />

            <View
                android:id="@+id/userview"
                android:layout_width="277dp"
                android:layout_height="84dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="13dp"
                android:background="@drawable/rounded_all_corners"
                app:layout_constraintStart_toStartOf="@id/myInfo"
                app:layout_constraintTop_toBottomOf="@id/myInfo" />

            <TextView
                android:id="@+id/usercarnum"
                android:layout_width="110dp"
                android:layout_height="24dp"
                android:layout_marginStart="21dp"
                android:layout_marginTop="18dp"
                android:text=""
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="@id/userview"
                app:layout_constraintTop_toTopOf="@id/userview" />

            <TextView
                android:id="@+id/timetext"
                android:layout_width="100dp"
                android:layout_height="18dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="12dp"
                android:textSize="12sp"
                android:textStyle="normal"
                app:layout_constraintStart_toStartOf="@id/usercarnum"
                app:layout_constraintTop_toBottomOf="@id/usercarnum" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/userimage"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginStart="23dp"
                android:layout_marginTop="0dp"
                android:src="@drawable/user"
                app:layout_constraintStart_toEndOf="@id/userview"
                app:layout_constraintTop_toTopOf="@id/userview" />

            <android.widget.Button
                android:id="@+id/manageprofile"
                android:layout_width="80dp"
                android:layout_height="18dp"
                android:layout_marginStart="-10dp"
                android:layout_marginTop="6dp"
                android:background="@drawable/rounded_all_corners"
                android:text="프로필 편집"
                android:textColor="@color/black"
                android:textSize="7dp"
                app:layout_constraintStart_toStartOf="@id/userimage"
                app:layout_constraintTop_toBottomOf="@id/userimage" />

            <TextView
                android:id="@+id/maptextview"
                android:layout_width="44dp"
                android:layout_height="30dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="35dp"
                android:text="지도"
                android:textColor="@color/black"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="@id/userview"
                app:layout_constraintTop_toBottomOf="@id/userview" />

            <androidx.fragment.app.FragmentContainerView
                android:id="@+id/mapimage"
                android:name="com.naver.maps.map.MapFragment"
                android:layout_width="370dp"
                android:layout_height="337dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="11dp"
                app:layout_constraintStart_toStartOf="@id/maptextview"
                app:layout_constraintTop_toBottomOf="@id/maptextview" />

            <android.widget.Button
                android:id="@+id/transparentbutton"
                android:layout_width="370dp"
                android:layout_height="337dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:background="@android:color/transparent"
                app:layout_constraintStart_toStartOf="@id/mapimage"
                app:layout_constraintTop_toTopOf="@id/mapimage" />

            <android.widget.Button
                android:id="@+id/inbutton"
                android:layout_width="173dp"
                android:layout_height="90dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="18dp"
                android:text="입차"
                app:layout_constraintStart_toStartOf="@id/mapimage"
                app:layout_constraintTop_toBottomOf="@id/mapimage" />

            <android.widget.Button
                android:id="@+id/outbutton"
                android:layout_width="173dp"
                android:layout_height="90dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="0dp"
                android:text="출차"
                app:layout_constraintStart_toEndOf="@id/inbutton"
                app:layout_constraintTop_toTopOf="@id/inbutton" />

            <View
                android:id="@+id/feeview"
                android:layout_width="370dp"
                android:layout_height="78dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="72dp"
                android:background="@drawable/rounded_all_corners"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@id/inbutton"
                app:layout_constraintTop_toBottomOf="@id/inbutton" />

            <ImageView
                android:id="@+id/parkinglogo"
                android:layout_width="25dp"
                android:layout_height="30dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="24dp"
                android:src="@drawable/parking_logo"
                app:layout_constraintStart_toStartOf="@id/feeview"
                app:layout_constraintTop_toTopOf="@id/feeview" />

            <TextView
                android:id="@+id/expectedfee"
                android:layout_width="79dp"
                android:layout_height="24dp"
                android:layout_marginStart="21dp"
                android:layout_marginTop="27dp"
                android:text="예상 요금"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/parkinglogo"
                app:layout_constraintTop_toTopOf="@id/feeview" />

            <TextView
                android:id="@+id/parkingfee"
                android:layout_width="150dp"
                android:layout_height="24dp"
                android:layout_marginStart="43dp"
                android:layout_marginTop="0dp"
                android:text="0원"
                app:layout_constraintStart_toEndOf="@id/expectedfee"
                app:layout_constraintTop_toTopOf="@id/expectedfee" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>