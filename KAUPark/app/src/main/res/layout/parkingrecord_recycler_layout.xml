<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="336dp"
    android:layout_height="74dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/rounded_all_corners"
    android:id="@+id/recycler"
    >

    <ImageView
        android:id="@+id/imageview_car_gif"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginTop="19dp"
        android:layout_marginStart="19dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <ImageView
        android:id="@+id/imageview_calender"
        android:layout_width="15dp"
        android:layout_height="16.13dp"
        android:layout_marginTop="0dp"
        android:layout_marginStart="44dp"
        app:layout_constraintTop_toTopOf="@id/imageview_car_gif"
        app:layout_constraintStart_toEndOf="@id/imageview_car_gif"
        android:src="@drawable/calender"/>
    
    <TextView
        android:id="@+id/textview_time"
        android:layout_width="77dp"
        android:layout_height="15dp"
        android:layout_marginTop="0dp"
        android:layout_marginStart="9dp"
        app:layout_constraintTop_toTopOf="@id/imageview_calender"
        app:layout_constraintStart_toEndOf="@id/imageview_calender"
        android:text="2024-11-16"
        android:textColor="#807E7E"
        android:textSize="12sp"
        android:fontFamily="@font/pretendard_semibold"/>

    <ImageView
        android:id="@+id/imageview_won"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="6.87dp"
        android:layout_marginStart="0dp"
        app:layout_constraintTop_toBottomOf="@id/imageview_calender"
        app:layout_constraintStart_toStartOf="@+id/imageview_calender"
        android:src="@drawable/moneysign"/>
    
    <TextView
        android:id="@+id/textview_won"
        android:layout_width="48dp"
        android:layout_height="17dp"
        android:layout_marginTop="0dp"
        android:layout_marginStart="0dp"
        app:layout_constraintTop_toTopOf="@id/imageview_won"
        app:layout_constraintStart_toStartOf="@id/textview_time"
        android:text="12000"
        android:textColor="#807E7E"
        android:textSize="12sp"
        android:fontFamily="@font/pretendard_semibold"/>
    
    <ImageView
        android:id="@+id/imageview_clock"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginTop="0dp"
        android:layout_marginStart="9dp"
        app:layout_constraintTop_toTopOf="@id/imageview_calender"
        app:layout_constraintStart_toEndOf="@id/textview_time"
        android:src="@drawable/clock"/>

    <TextView
        android:id="@+id/textview_clock"
        android:layout_width="54dp"
        android:layout_height="13dp"
        android:layout_marginTop="0dp"
        android:layout_marginStart="9dp"
        app:layout_constraintTop_toTopOf="@id/imageview_clock"
        app:layout_constraintStart_toEndOf="@+id/imageview_clock"
        android:text="3h 13min"
        android:textColor="#807E7E"
        android:textSize="12sp"
        android:fontFamily="@font/pretendard_semibold"/>



</androidx.constraintlayout.widget.ConstraintLayout>